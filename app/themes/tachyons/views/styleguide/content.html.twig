<article class="mt5">
  <h3 class="f6 ttu fw6 mt0 mb3 bb pb2">Headings</h3>
  <div class="row">
    <div class="col-2 mb3">
      Element
    </div>
    <div class="col-2 mb3">
      Size
    </div>
    <div class="col mb3">
      Example
    </div>
  </div>
  {% set rows = {"h1":"f1", "h2":"f2", "h3":"f3", "h4":"f4", "h5":"f5", "h6":"f6"} %}
  {% for element, size in rows %}
  <div class="row items-center">
    <div class="col-2 mb3">
      <code class="f6 truncate mid-gray">{{ element }}</code>
    </div>
    <div class="col-2 mb3">
      <code class="f6 truncate mid-gray">{{ size }}</code>
    </div>
    <div class="col mb3">
      <{{ element }}>Heading element</{{ element }}>
    </div>
  </div>
  {% endfor %}
</article>

