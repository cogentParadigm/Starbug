{% if response.path != "home" %}
<div class="pg-opt">
  <div class="container">
      <div class="row">
          <div class="col-md-6">
              <h2>{{ response.title }}</h2>
          </div>
          <div class="col-md-6">
              <ol class="breadcrumb">
                  <li><a href="{{ helper("url").build("") }}">Home</a></li>
                  <li class="active">{{ response.title }}</li>
              </ol>
          </div>
      </div>
  </div>
</div>
{% endif %}
